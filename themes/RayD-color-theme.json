{
    "name": "RayD",
    "type": "dark",
    "colors": {
      "editor.background": "#000814", // Darkest blue
      "editor.foreground": "#D4D4D4",
      "editorLineNumber.foreground": "#4B6478", // Pastel grey-blue
      "editorLineNumber.activeForeground": "#A8DADC", // Pastel teal
      "editorCursor.foreground": "#F4A261", // Pastel orange
      "editor.selectionBackground": "#001D3D", // Slightly lighter darkest blue
      "editor.wordHighlightBackground": "#00356608", // Deeper blue for word highlights
      "editor.findMatchBackground": "#778DA9",
      "editor.findMatchHighlightBackground": "#3D5A8008",
      "editorBracketMatch.background": "#003566",
      "editorBracketMatch.border": "#F4A261",
      "editorGutter.background": "#000814",
      "activityBar.background": "#000814",
      "activityBar.foreground": "#A8DADC", // Pastel teal
      "activityBarBadge.background": "#E63946", // Pastel red
      "activityBarBadge.foreground": "#F1FAEE", // Pastel white
      "sideBar.background": "#000814",
      "sideBar.foreground": "#D4D4D4",
      "statusBar.background": "#000814",
      "statusBar.foreground": "#A8DADC",
      "titleBar.activeBackground": "#000814",
      "titleBar.activeForeground": "#A8DADC"
    },
    "tokenColors": [
      {
        "scope": ["comment"],
        "settings": {
          "foreground": "#778DA9" // Pastel grey-blue for comments
        }
      },
      {
        "scope": ["keyword", "storage", "type.primitive"],
        "settings": {
          "foreground": "#C084FC" // Pastel purple for int, char
        }
      },
      {
        "scope": ["entity.name.function"],
        "settings": {
          "foreground": "#A8DADC" // Pastel teal for main
        }
      },
      {
        "scope": ["variable.parameter", "keyword.operator.comparison", "keyword.operator.assignment"],
        "settings": {
          "foreground": "#F1FAEE" // Pastel white for argc, <, =
        }
      },
      {
        "scope": ["constant.numeric"],
        "settings": {
          "foreground": "#FFE194" // Pastel yellow for digits
        }
      },
      {
        "scope": ["string"],
        "settings": {
          "foreground": "#F4A261" // Pastel orange for strings
        }
      },
      {
        "scope": ["variable", "identifier"],
        "settings": {
          "foreground": "#D4D4D4" // Neutral for variables
        }
      },
      {
        "scope": ["function"],
        "settings": {
          "foreground": "#84A59D" // Pastel green-blue for functions
        }
      },
      {
        "scope": ["constant"],
        "settings": {
          "foreground": "#F28482" // Pastel coral for constants
        }
      },
      {
        "scope": ["type", "class"],
        "settings": {
          "foreground": "#98C1D9" // Pastel cyan for types and classes
        }
      }
    ]
  }
  