{
  "name": "RayD",
  "type": "dark",
  "colors": {
    "editor.background": "#1C1C1C",
    "editor.foreground": "#D1D1D1",
    "editor.lineHighlightBackground": "#2E2E2E",
    "editor.selectionBackground": "#4D6D88",
    "editorCursor.foreground": "#E0E0E0",

    "activityBar.background": "#2D2D2D",
    "activityBar.foreground": "#D1D1D1",
    "activityBarBadge.background": "#4E727C",
    "activityBarBadge.foreground": "#E0E0E0",

    "sideBar.background": "#2D2D2D",
    "sideBar.foreground": "#B0B0B0",
    "sideBarSectionHeader.background": "#4A4A4A",
    "sideBarSectionHeader.foreground": "#D1D1D1",

    "titleBar.activeBackground": "#2D2D2D",
    "titleBar.activeForeground": "#D1D1D1",

    "statusBar.background": "#3A3A3A",
    "statusBar.foreground": "#D1D1D1",
    "statusBar.debuggingBackground": "#7A9E7D",
    "statusBar.debuggingForeground": "#E0E0E0",

    "tab.activeBackground": "#2D2D2D",
    "tab.inactiveBackground": "#353535",
    "tab.activeForeground": "#D1D1D1",
    "tab.inactiveForeground": "#8A8A8A",

    "terminal.background": "#1C1C1C",
    "terminal.foreground": "#D1D1D1",
    "terminalCursor.background": "#E0E0E0",
    "terminalCursor.foreground": "#E0E0E0",

    "panel.background": "#2D2D2D",
    "panel.border": "#3A3A3A",

    "editorGroupHeader.tabsBackground": "#2D2D2D",
    "editorGroupHeader.tabsBorder": "#3A3A3A"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#B4A594",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#FF9A6A"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#A3B9D4"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#A3B9D4"
      }
    },
    {
      "scope": "constant",
      "settings": {
        "foreground": "#A3B9D4"
      }
    },
    {
      "scope": "function",
      "settings": {
        "foreground": "#A3B9D4"
      }
    },
    {
      "scope": "type",
      "settings": {
        "foreground": "#B28DFF"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#A3B9D4"
      }
    },
    {
      "scope": "operator",
      "settings": {
        "foreground": "#A3B9D4"
      }
    }
  ]
}
